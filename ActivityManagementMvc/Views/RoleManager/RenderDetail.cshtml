@model ActivityManagement.ViewModels.RoleManager.RolesViewModel

<div class="Detail">



    <div class="form-group">

        <label asp-for="Name" class="col-form-label"></label>:
        <span>
            @Html.DisplayFor(model => model.Name)

        </span>
    </div>
    <div class="form-group">

        <label asp-for="UsersCount" class="col-form-label"></label>:
        <span>
            @Html.DisplayFor(model => model.UsersCount)
        </span>
    </div>
    <div class="form-group mt-2">

        <label asp-for="Description" class="col-form-label"></label>:
        <span>
            @Html.DisplayFor(model => model.Description)
        </span>

    </div>
</div>







